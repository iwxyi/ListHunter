{
	"placeholder": "搜索端口号",
	"search_types": [
		{
			"key_exp": "^(\\d+)$",
			"search_exp": "netstat -pel | grep :%1"
		},
		{
			"key_exp": "^(.+)$",
			"search_exp": "netstat -pel | grep %1"
		},
		{
			"key_exp": "^$",
			"search_exp": "netstat -pel"
		}
	],
	"result_titles":[
		"协议",
		"本地地址",
		"外部地址",
		"状态",
		"User",
		"Inode",
		"PID/Program name"
	],
	"result_lines":[
		{
			"expression": "^\\s*(\\w+)\\s+\\d+\\s+\\d+\\s+([\\w\\.:]+)\\s+([\\w\\.:\\*]+)\\s+(\\w+)\\s+(\\S+)\\s+(\\d+)\\s+(-|\\d+/.+).*$",
			"actions": [
				{
					"name": "Stop Application",
					"cmd": "kill -9 %7",
					"exp": "",
					"refresh": true
				}
			]
		},
		{
			"expression": "^\\s*Proto.+$",
			"ignore": true
		}
	]
}
